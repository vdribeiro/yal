module max_array
{

function max=maxarray(a[]) {

	max = a[0];

	i=1;
	while(i < a.size) {
		if(max < a[i]) {
			max = a[i];
		}
		i=i+1;
	}

	io.print("max: ", max);
}

 function main() {
 		  a=[10];  // creates an array with 10 elements
 		  i=0;
		  while(i < 10) {
		     a[i]=i;
		     i=i+1;
          }
 		  max=maxarray(a);
 }

}